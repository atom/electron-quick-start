{
  "name": "cbgui",
  "version": "0.1.0",
  "description": "An electron frontend for scripts maintained by Chris B",
  "main": "main.js",
  "build": {
    "appId": "cbgui.gurucomputing.com.au",
    "win": {
      "target": "portable",
      "icon": "assets/icons/icon.ico",
      "extraFiles": "scripts/**",
      "requestedExecutionLevel": "requireAdministrator"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "Powershell"
  ],
  "author": "Christopher Bisset",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.3.1",
    "electron-builder": "^22.7.0"
  },
  "dependencies": {
    "@json-editor/json-editor": "^2.2.1",
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "selectize": "^0.12.6"
  }
}
